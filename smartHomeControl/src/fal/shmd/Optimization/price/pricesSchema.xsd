<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/pricesSchema" xmlns:tns="http://www.example.org/pricesSchema" elementFormDefault="qualified">

    <element name="priceSet">
    	<complexType>
    		<sequence>
    			<element name="prices" maxOccurs="unbounded" minOccurs="1">
    				<complexType>
    					<sequence>
    						<element name="price" type="int" maxOccurs="unbounded" minOccurs="1"></element>
    					</sequence>
    					<attribute name="interval" type="int"></attribute>
    				</complexType>
    			</element>
    		</sequence>
    	</complexType>
    </element>
</schema>
